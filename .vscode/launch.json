   "version": "0.2.0",
   "configurations": [
      {
         "name": "(gdb) ./woody-woodpacker",
         "type": "cppdbg",
         "request": "launch",
         "program": "${workspaceFolder}/woody-woodpacker",
         "args": [
            "test/9.3-pie-pwd",
            "-t",
            "key_param"
         ],
         "stopAtEntry": false,
         "cwd": "${workspaceFolder}",
         "environment": [],
         "externalConsole": false,
         "MIMode": "gdb",
         "setupCommands": [
             {
                 "description": "Enable pretty-printing for gdb",
                 "text": "-enable-pretty-printing",
                 "ignoreFailures": true
             }
         ]
      }
   ]
}